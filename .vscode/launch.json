{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Init",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/omni-server/main.go",
            "args": [
                "-conf",
                "${workspaceFolder}/build/conf.yaml",
                "-init"
            ]
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/omni-server/main.go",
            "args": [
                "-conf",
                "${workspaceFolder}/build/conf.yaml"
            ]
        },
        {
            "name": "Python: File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "justMyCode": true
        }
    ]
}